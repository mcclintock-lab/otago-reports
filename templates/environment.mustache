              <tr>
                <td>{{HAB_TYPE}}</td>
                <td>{{SIZE_SQKM}}</td>
                <td>{{PERC}}</td>
                {{#isMPA}}
                 
                  {{#isCollection}} <!--<td>{{REPRESENT}}</td>--><td>{{REPLIC}}</td><td>{{CONN}}</td>{{/isCollection}}
                {{/isMPA}}
              </tr>
              {{/coastal_hab_types}}
            {{/hasCoastalHabTypes}}
            {{^hasCoastalHabTypes}}
              <tr>
                <td colspan="{{#isCollection}}5{{/isCollection}}{{^isCollection}}4{{/isCollection}}">
                  <i>There are no coastal habitat types.</i>
                </td>
              </tr>
            {{/hasCoastalHabTypes}}
